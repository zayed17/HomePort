{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://api.cartfurnish.shop/api/$1"
    }
  ],
  "headers": [
    {
      "source": "/about.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=1, stale-while-revalidate=59"
        }
      ]
    }
  ],
  "routes": [
    {
      "src": "/[^.]+",
      "dest": "/",
      "status": 200
    }
  ]
}
